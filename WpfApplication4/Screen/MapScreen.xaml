<Window x:Class="UMA24.Screen.MapScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MapScreen"  Name="MapWindow" WindowStartupLocation="CenterScreen" WindowState="Maximized" WindowStyle="None">
    <StackPanel>
        <WebBrowser Height="970" HorizontalAlignment="Stretch" Name="MapBrowser" />
        <Button Content="지 도 닫 기" Height="75" HorizontalAlignment="Stretch"  Name="btnHide" VerticalAlignment="Top"  Click="btnHide_Click" Background="{StaticResource {x:Static SystemColors.HighlightBrushKey}}" FontSize="40" />
    </StackPanel>
</Window>
